<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<!-- maven commend like: -->
<!-- org.mybatis.generator:mybatis-generator-maven-plugin:1.3.1:generate -Dmybatis.generator.overwrite=true -->
<generatorConfiguration>
    <context id="smart_community" targetRuntime="MyBatis3">

        <plugin type="org.mybatis.generator.plugins.EqualsHashCodePlugin" />
        <plugin type="org.mybatis.generator.plugins.SerializablePlugin" />
        <plugin type="org.mybatis.generator.plugins.CaseInsensitiveLikePlugin" />

        <!-- 配置如何生成注释信息-->
        <commentGenerator>
            <property name="suppressAllComments" value="true" />
            <property name="suppressDate" value="true" />
        </commentGenerator>
        <!-- 配置数据库-->
        <jdbcConnection driverClass="com.mysql.cj.jdbc.Driver"
                        connectionURL="jdbc:mysql://localhost:3306/tt?useSSL=false&amp;useUnicode=false&amp;characterEncoding=UTF8&amp;serverTimezone=UTC"
                        userId="root" password="123456">
        </jdbcConnection>

        <!-- 指定JDBC类型和java类型如何转换-->
        <javaTypeResolver>
            <property name="forceBigDecimals" value="false" />
        </javaTypeResolver>
        <!-- 生成实体类-->
        <javaModelGenerator targetPackage="com.zust.dao.User.bean"
                            targetProject="./src/main/java">
            <property name="enableSubPackages" value="false" />
            <property name="trimStrings" value="true" />
        </javaModelGenerator>
        <!-- xml-->
        <sqlMapGenerator targetPackage="com.zust.dao.mapping"
                         targetProject="./src/main/resources">
            <property name="enableSubPackages" value="false" />
        </sqlMapGenerator>
        <!-- mapper-->
        <javaClientGenerator type="XMLMAPPER"
                             targetPackage="com.zust.dao.User" targetProject="./src/main/java">
            <property name="enableSubPackages" value="false" />
        </javaClientGenerator>

        <!-- database table-->
        <table schema="tt" tableName="yu"
               enableCountByExample="false"
               enableUpdateByExample="false" enableDeleteByExample="true"
               enableSelectByExample="true" selectByExampleQueryId="false">
            <property name="constructorBased" value="false" />
            <property name="useActualColumnNames" value="false" />
            <property name="ignoreQualifiersAtRuntime" value="true" />
            <generatedKey column="id" sqlStatement="MySql" identity="true" />
        </table>
    </context>
</generatorConfiguration>